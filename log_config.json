{
    "version": 1,
    "formatters": {
        "root": {
            "format": "[%(asctime)s][%(levelname)s][%(module)s] %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "root"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": ["console"]
    }
}
